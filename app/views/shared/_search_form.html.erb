<%= form_with scope: :search, url: search_path, method: :post do |f| %>
    <%= f.text_field :text %>
    <%= f.select :search_model,
      {
        "Book" => "Book",
        "User" => "User",
      },
      {
        selected: 1,
      }
    %>
    <%= f.select :search_type,
      {
        "完全一致" => "完全一致",
        "前方一致" => "前方一致",
        "後方一致" => "後方一致",
        "部分一致" => "部分一致",
      },
      {
        selected: 1,
      }
    %>
    <%= f.submit "検索" %>
<% end %>
